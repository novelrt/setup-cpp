{
  "problemMatcher": [
    {
      "owner": "msvc",
      "pattern": [
        {
          "regexp": "^(?:\\s+\\d+\\>)?([^\\s].*)\\((?:(\\d+)(?:,(\\d+)(?:,\\d+,\\d+)?)?)\\)\\s*:\\s+(?:fatal\\s+)?(error|warning|info)\\s+(\\w+\\d+)\\s*:\\s*(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}
